{
  "name": "swago",
  "version": "1.1.1",
  "description": "A useful swagger viewer",
  "main": "dist/main_bundle.js",
  "scripts": {
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "start:server": "node server.js",
    "start:app": "electron dist/main_bundle.js",
    "start": "run-p start:server start:app",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "full": "webpack --config webpack.config.js --mode production & electron-builder"
  },
  "build": {
    "appId": "geoffcox.swago",
    "directories": {
      "output": "electron"
    }
  },
  "author": "Geoff Cox",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-measure": "^2.0.6",
    "@types/recoil": "0.0.1",
    "@types/styled-components": "4.1.8",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.4.2",
    "electron": "^15.5.5",
    "electron-builder": "^22.4.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "typescript": "3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "typescript-plugin-styled-components": "^1.4.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@geoffcox/react-splitter": "^2.0.1",
    "csx": "^10.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-measure": "^2.3.0",
    "recoil": "0.0.10",
    "styled-components": "^5.1.1",
    "swagger-parser": "^8.0.4"
  }
}
